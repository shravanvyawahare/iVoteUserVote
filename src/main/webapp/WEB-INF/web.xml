<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>hello</servlet-name>
  	<display-name>hello</display-name>
  	<description></description>
  	<servlet-class>com.controller.hello</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>showPhone</servlet-name>
  	<display-name>showPhone</display-name>
  	<description></description>
  	<servlet-class>com.controller.showPhone</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>hello</servlet-name>
  	<url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>showPhone</servlet-name>
  	<url-pattern>/showPhone</url-pattern>
  </servlet-mapping>
</web-app>
